<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:maximApp"
            xmlns:shared="clr-namespace:maximApp.SharedUI.Components;assembly=maximApp.SharedUI"
            x:Class="maximApp.MainPage">

    <BlazorWebView x:Name="blazorWebView" HostPage="wwwroot/index.html">
        <BlazorWebView.RootComponents>
            <RootComponent Selector="#app" ComponentType="{x:Type shared:MauiRoot}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>

</ContentPage>
